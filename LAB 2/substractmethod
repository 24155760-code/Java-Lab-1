// Class with overloaded subtract methods
class Subtraction {

    // subtract two integers
    int subtract(int a, int b) {
        return a - b;
    }

    // subtract three integers
    int subtract(int a, int b, int c) {
        return a - b - c;
    }

    // subtract two double values
    double subtract(double a, double b) {
        return a - b;
    }
}

// Driver class
public class SubtractDemo {
    public static void main(String[] args) {

        Subtraction obj = new Subtraction();

        System.out.println("Subtract two integers: " + obj.subtract(10, 5));
        System.out.println("Subtract three integers: " + obj.subtract(20, 5, 3));
        System.out.println("Subtract two doubles: " + obj.subtract(15.5, 5.2));
    }
}
